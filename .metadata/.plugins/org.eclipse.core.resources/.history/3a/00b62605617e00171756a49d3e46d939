package com.globalexpress.web.entity;

import java.io.Serializable;
import java.util.Date;

public class OrderInfo implements Serializable{
	private static final long serialVersionUID = -2495647866689707966L;
	
	private Long id;//记录编号ID
	
	private Long shopId;//店铺编号

	private Long orderNum;//订单编号
	
	private String carrierId;//物流公司编号或名称

	private Long trackingNum;//物流单号

	private Long consignorId;//发件人名称

	private Long consignorTel;//发件人电话

	private String consignorAddress;//发件地址

	private String consignorSignature;//发件人签名

	private String consigneeId;//收件人名称

	private Long consigneeTel;//收件人电话

	private String consigneeAddress;//收件人地址

	private String consigneeSignature;//收件人签名

	private Integer packageWeight;//包裹重量

	private Integer totalShippingFee;//订单总运费

	private Integer supportValue;//

	private Byte orderType;//订单类型

	private String orderLabel;//订单标签

	private String orderKeyword;//订单关键词

	private String reserved1;//自定义内容

	private String note;//备注

	private Long ownerId;//用户主账户ID

	private Long operatorId;//该记录操作人

	private Boolean authorization;//该记录是否被授权

	private Boolean apply;//该记录是否已被应用或调用

	private Date gmtCreate;//记录创建时间

	private Date gmtModified;//记录修改时间

	private String buyerWord;//买家留言
	
	private String sellerWord;//卖家备注
	
	private Byte invoiceType;//发票类型
	
	private String invoiceTitle;//发票抬头
	
	private Long taxpayerId;//机构统一社会信用代码（纳税人识别号）
	
	private String invoiceContent;//发票内容
	
	private Byte orderPaid;//订单付款标记
	
	private Date orderPay;//付款时间
	
	private Date refundApply;//申请退款时间
	
	private Byte refundCompleted;//已退款标记
	
	private Date refundComplete;//退款完成时间
	
	private Byte orderClosed;//已关闭标记
	
	private Date orderClose;//订单关闭时间
	
	private Byte orderDeparted;//订单已发货标记
	
	private Date orderDeparture;//发货时间
	
	private Byte orderCompleted;//订单已完成标记
	
	private Date orderComplete;//订单完成时间
	
	private Byte orderCod;//是否货到付款
	
	private Date shippingfeeCod;//是否运费到付
	
	public OrderInfo() {
		
	}

	public OrderInfo(Long id, Long shopId, Long orderNum, String carrierId, Long trackingNum, Long consignorId,
			Long consignorTel, String consignorAddress, String consignorSignature, String consigneeId,
			Long consigneeTel, String consigneeAddress, String consigneeSignature, Integer packageWeight,
			Integer totalShippingFee, Integer supportValue, Byte orderType, String orderLabel, String orderKeyword,
			String reserved1, String note, Long ownerId, Long operatorId, Boolean authorization, Boolean apply,
			Date gmtCreate, Date gmtModified, String buyerWord, String sellerWord, Byte invoiceType,
			String invoiceTitle, Long taxpayerId, String invoiceContent, Byte orderPaid, Date orderPay,
			Date refundApply, Byte refundCompleted, Date refundComplete, Byte orderClosed, Date orderClose,
			Byte orderDeparted, Date orderDeparture, Byte orderCompleted, Date orderComplete, Byte orderCod,
			Date shippingfeeCod) {
		super();
		this.id = id;
		this.shopId = shopId;
		this.orderNum = orderNum;
		this.carrierId = carrierId;
		this.trackingNum = trackingNum;
		this.consignorId = consignorId;
		this.consignorTel = consignorTel;
		this.consignorAddress = consignorAddress;
		this.consignorSignature = consignorSignature;
		this.consigneeId = consigneeId;
		this.consigneeTel = consigneeTel;
		this.consigneeAddress = consigneeAddress;
		this.consigneeSignature = consigneeSignature;
		this.packageWeight = packageWeight;
		this.totalShippingFee = totalShippingFee;
		this.supportValue = supportValue;
		this.orderType = orderType;
		this.orderLabel = orderLabel;
		this.orderKeyword = orderKeyword;
		this.reserved1 = reserved1;
		this.note = note;
		this.ownerId = ownerId;
		this.operatorId = operatorId;
		this.authorization = authorization;
		this.apply = apply;
		this.gmtCreate = gmtCreate;
		this.gmtModified = gmtModified;
		this.buyerWord = buyerWord;
		this.sellerWord = sellerWord;
		this.invoiceType = invoiceType;
		this.invoiceTitle = invoiceTitle;
		this.taxpayerId = taxpayerId;
		this.invoiceContent = invoiceContent;
		this.orderPaid = orderPaid;
		this.orderPay = orderPay;
		this.refundApply = refundApply;
		this.refundCompleted = refundCompleted;
		this.refundComplete = refundComplete;
		this.orderClosed = orderClosed;
		this.orderClose = orderClose;
		this.orderDeparted = orderDeparted;
		this.orderDeparture = orderDeparture;
		this.orderCompleted = orderCompleted;
		this.orderComplete = orderComplete;
		this.orderCod = orderCod;
		this.shippingfeeCod = shippingfeeCod;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public Long getShopId() {
		return shopId;
	}

	public void setShopId(Long shopId) {
		this.shopId = shopId;
	}

	public Long getOrderNum() {
		return orderNum;
	}

	public void setOrderNum(Long orderNum) {
		this.orderNum = orderNum;
	}

	public String getCarrierId() {
		return carrierId;
	}

	public void setCarrierId(String carrierId) {
		this.carrierId = carrierId;
	}

	public Long getTrackingNum() {
		return trackingNum;
	}

	public void setTrackingNum(Long trackingNum) {
		this.trackingNum = trackingNum;
	}

	public Long getConsignorId() {
		return consignorId;
	}

	public void setConsignorId(Long consignorId) {
		this.consignorId = consignorId;
	}

	public Long getConsignorTel() {
		return consignorTel;
	}

	public void setConsignorTel(Long consignorTel) {
		this.consignorTel = consignorTel;
	}

	public String getConsignorAddress() {
		return consignorAddress;
	}

	public void setConsignorAddress(String consignorAddress) {
		this.consignorAddress = consignorAddress;
	}

	public String getConsignorSignature() {
		return consignorSignature;
	}

	public void setConsignorSignature(String consignorSignature) {
		this.consignorSignature = consignorSignature;
	}

	public String getConsigneeId() {
		return consigneeId;
	}

	public void setConsigneeId(String consigneeId) {
		this.consigneeId = consigneeId;
	}

	public Long getConsigneeTel() {
		return consigneeTel;
	}

	public void setConsigneeTel(Long consigneeTel) {
		this.consigneeTel = consigneeTel;
	}

	public String getConsigneeAddress() {
		return consigneeAddress;
	}

	public void setConsigneeAddress(String consigneeAddress) {
		this.consigneeAddress = consigneeAddress;
	}

	public String getConsigneeSignature() {
		return consigneeSignature;
	}

	public void setConsigneeSignature(String consigneeSignature) {
		this.consigneeSignature = consigneeSignature;
	}

	public Integer getPackageWeight() {
		return packageWeight;
	}

	public void setPackageWeight(Integer packageWeight) {
		this.packageWeight = packageWeight;
	}

	public Integer getTotalShippingFee() {
		return totalShippingFee;
	}

	public void setTotalShippingFee(Integer totalShippingFee) {
		this.totalShippingFee = totalShippingFee;
	}

	public Integer getSupportValue() {
		return supportValue;
	}

	public void setSupportValue(Integer supportValue) {
		this.supportValue = supportValue;
	}

	public Byte getOrderType() {
		return orderType;
	}

	public void setOrderType(Byte orderType) {
		this.orderType = orderType;
	}

	public String getOrderLabel() {
		return orderLabel;
	}

	public void setOrderLabel(String orderLabel) {
		this.orderLabel = orderLabel;
	}

	public String getOrderKeyword() {
		return orderKeyword;
	}

	public void setOrderKeyword(String orderKeyword) {
		this.orderKeyword = orderKeyword;
	}

	public String getReserved1() {
		return reserved1;
	}

	public void setReserved1(String reserved1) {
		this.reserved1 = reserved1;
	}

	public String getNote() {
		return note;
	}

	public void setNote(String note) {
		this.note = note;
	}

	public Long getOwnerId() {
		return ownerId;
	}

	public void setOwnerId(Long ownerId) {
		this.ownerId = ownerId;
	}

	public Long getOperatorId() {
		return operatorId;
	}

	public void setOperatorId(Long operatorId) {
		this.operatorId = operatorId;
	}

	public Boolean getAuthorization() {
		return authorization;
	}

	public void setAuthorization(Boolean authorization) {
		this.authorization = authorization;
	}

	public Boolean getApply() {
		return apply;
	}

	public void setApply(Boolean apply) {
		this.apply = apply;
	}

	public Date getGmtCreate() {
		return gmtCreate;
	}

	public void setGmtCreate(Date gmtCreate) {
		this.gmtCreate = gmtCreate;
	}

	public Date getGmtModified() {
		return gmtModified;
	}

	public void setGmtModified(Date gmtModified) {
		this.gmtModified = gmtModified;
	}

	public String getBuyerWord() {
		return buyerWord;
	}

	public void setBuyerWord(String buyerWord) {
		this.buyerWord = buyerWord;
	}

	public String getSellerWord() {
		return sellerWord;
	}

	public void setSellerWord(String sellerWord) {
		this.sellerWord = sellerWord;
	}

	public Byte getInvoiceType() {
		return invoiceType;
	}

	public void setInvoiceType(Byte invoiceType) {
		this.invoiceType = invoiceType;
	}

	public String getInvoiceTitle() {
		return invoiceTitle;
	}

	public void setInvoiceTitle(String invoiceTitle) {
		this.invoiceTitle = invoiceTitle;
	}

	public Long getTaxpayerId() {
		return taxpayerId;
	}

	public void setTaxpayerId(Long taxpayerId) {
		this.taxpayerId = taxpayerId;
	}

	public String getInvoiceContent() {
		return invoiceContent;
	}

	public void setInvoiceContent(String invoiceContent) {
		this.invoiceContent = invoiceContent;
	}

	public Byte getOrderPaid() {
		return orderPaid;
	}

	public void setOrderPaid(Byte orderPaid) {
		this.orderPaid = orderPaid;
	}

	public Date getOrderPay() {
		return orderPay;
	}

	public void setOrderPay(Date orderPay) {
		this.orderPay = orderPay;
	}

	public Date getRefundApply() {
		return refundApply;
	}

	public void setRefundApply(Date refundApply) {
		this.refundApply = refundApply;
	}

	public Byte getRefundCompleted() {
		return refundCompleted;
	}

	public void setRefundCompleted(Byte refundCompleted) {
		this.refundCompleted = refundCompleted;
	}

	public Date getRefundComplete() {
		return refundComplete;
	}

	public void setRefundComplete(Date refundComplete) {
		this.refundComplete = refundComplete;
	}

	public Byte getOrderClosed() {
		return orderClosed;
	}

	public void setOrderClosed(Byte orderClosed) {
		this.orderClosed = orderClosed;
	}

	public Date getOrderClose() {
		return orderClose;
	}

	public void setOrderClose(Date orderClose) {
		this.orderClose = orderClose;
	}

	public Byte getOrderDeparted() {
		return orderDeparted;
	}

	public void setOrderDeparted(Byte orderDeparted) {
		this.orderDeparted = orderDeparted;
	}

	public Date getOrderDeparture() {
		return orderDeparture;
	}

	public void setOrderDeparture(Date orderDeparture) {
		this.orderDeparture = orderDeparture;
	}

	public Byte getOrderCompleted() {
		return orderCompleted;
	}

	public void setOrderCompleted(Byte orderCompleted) {
		this.orderCompleted = orderCompleted;
	}

	public Date getOrderComplete() {
		return orderComplete;
	}

	public void setOrderComplete(Date orderComplete) {
		this.orderComplete = orderComplete;
	}

	public Byte getOrderCod() {
		return orderCod;
	}

	public void setOrderCod(Byte orderCod) {
		this.orderCod = orderCod;
	}

	public Date getShippingfeeCod() {
		return shippingfeeCod;
	}

	public void setShippingfeeCod(Date shippingfeeCod) {
		this.shippingfeeCod = shippingfeeCod;
	}

	@Override
	public String toString() {
		return "OrderInfo [id=" + id + ", shopId=" + shopId + ", orderNum=" + orderNum + ", carrierId=" + carrierId
				+ ", trackingNum=" + trackingNum + ", consignorId=" + consignorId + ", consignorTel=" + consignorTel
				+ ", consignorAddress=" + consignorAddress + ", consignorSignature=" + consignorSignature
				+ ", consigneeId=" + consigneeId + ", consigneeTel=" + consigneeTel + ", consigneeAddress="
				+ consigneeAddress + ", consigneeSignature=" + consigneeSignature + ", packageWeight=" + packageWeight
				+ ", totalShippingFee=" + totalShippingFee + ", supportValue=" + supportValue + ", orderType="
				+ orderType + ", orderLabel=" + orderLabel + ", orderKeyword=" + orderKeyword + ", reserved1="
				+ reserved1 + ", note=" + note + ", ownerId=" + ownerId + ", operatorId=" + operatorId
				+ ", authorization=" + authorization + ", apply=" + apply + ", gmtCreate=" + gmtCreate
				+ ", gmtModified=" + gmtModified + ", buyerWord=" + buyerWord + ", sellerWord=" + sellerWord
				+ ", invoiceType=" + invoiceType + ", invoiceTitle=" + invoiceTitle + ", taxpayerId=" + taxpayerId
				+ ", invoiceContent=" + invoiceContent + ", orderPaid=" + orderPaid + ", orderPay=" + orderPay
				+ ", refundApply=" + refundApply + ", refundCompleted=" + refundCompleted + ", refundComplete="
				+ refundComplete + ", orderClosed=" + orderClosed + ", orderClose=" + orderClose + ", orderDeparted="
				+ orderDeparted + ", orderDeparture=" + orderDeparture + ", orderCompleted=" + orderCompleted
				+ ", orderComplete=" + orderComplete + ", orderCod=" + orderCod + ", shippingfeeCod=" + shippingfeeCod
				+ "]";
	}

	
}